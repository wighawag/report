<?xml version="1.0" encoding="utf-8" ?>
<project>
	<yoga-version>1</yoga-version>
	<compiler-version>2.0.9</compiler-version>
	
	<id>com.wighawag.report.Report</id>
	<version>0.1</version>
	
	<main>ReportUseTest</main>
	
	<sources>
		<source path="src" />
	</sources>
	
	<tests framework="munit" path="test" />
	
	<targets hxml="compile">
		
		<target name="swf" >
			<param line="-swf-version 11" />
		</target>
		
		<target name="cpp" />

		<target name="js" />		

	</targets>
	
	
	<config-file template="hxproj.template" output="Report_flash.hxproj" />
	<config-file template="sln.template" output="Report.sln" />

	<dependencies>
		<haxelib name="msignal" version="1.0.0" />
	</dependencies>
</project>
